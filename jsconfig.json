{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ES6",
    "moduleResolution": "node",
    "paths": {
      "lib/*": ["./lib/*"],
      "shared/*": ["./components/shared/*"],
      "constants/*": ["./constants/*"],
      "stylesheets/*": ["./stylesheets/*"],
      "components/*": ["./components/*"]
    }
  },
  "exclude": [
    "snapshots.js",
    "node_modules",
    "lib",
    ".tmp",
    "build",
    ".next"
  ]
}