{
  "presets": ["next/babel"],
  "env": {
    "development": {
      "plugins": [
        [
          "dotenv-import",
          {
            "whitelist": ["SITE_ENV"]
          }
        ]
      ]
    },
    "production": {
      "plugins": [
        [
          "dotenv-import", {
            "whitelist": [
              "SITE_ENV"
            ],
            "safe": false
          }
        ]
      ]
    }
  },
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "css": "./css",
          "static": "./static",
          "utilFunctions": "./utilFunctions",
          "components": "./components"
        }
      }
    ]
  ]
}
